<?xml version="1.0" encoding="utf-8"?>

<com.fabricmonde.salesapp.customscrollview.ScrollViewWithListener
    android:id="@+id/scroll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view"
        android:padding="16dp"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <View
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/graylight"
        android:layout_marginTop="20.7dp"/>

    <LinearLayout
        android:id="@+id/quality"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="20dp"
        android:orientation="vertical">

        <customfonts.MyTextView_Roboto_Regular
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/sfpro_display_bold"
            android:layout_gravity="center"
            android:text="Quality parameters"
            android:textSize="20sp"
            android:letterSpacing="0.0425"
            android:textColor="#000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:gravity="center"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="Width :   "
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />

            <com.moos.library.HorizontalProgressView
                android:id="@+id/progressbar_width"
                android:layout_width="200dp"
                android:layout_height="40dp"
                app:start_color="#3bb143"
                app:end_color="#0b6623"
                android:layout_gravity="center"
                app:track_width="20dp"
                app:corner_radius="10dp"
                app:end_progress="90"
                app:progressTextColor="#696969"
                app:isTracked="true"
                app:textMovedEnable="true"
                app:trackColor="#f4f4f4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="± 5"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="21dp"
            android:background="@color/graylight" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="Count "
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />

            <com.moos.library.HorizontalProgressView
                android:id="@+id/progressbar_count"
                android:layout_width="200dp"
                android:layout_height="40dp"
                app:start_color="#3bb143"
                app:end_color="#0b6623"
                android:layout_gravity="center"
                app:track_width="20dp"
                app:corner_radius="10dp"
                app:end_progress="80"
                app:progressTextColor="#696969"
                app:isTracked="true"
                app:textMovedEnable="true"
                app:trackColor="#f4f4f4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="± 5"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="21dp"
            android:background="@color/graylight" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="Construction"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />

            <com.moos.library.HorizontalProgressView
                android:id="@+id/progressbar_construction"
                android:layout_width="200dp"
                android:layout_height="40dp"
                app:start_color="#3bb143"
                app:end_color="#0b6623"
                android:layout_gravity="center"
                app:track_width="20dp"
                app:corner_radius="10dp"
                app:end_progress="65"
                app:progressTextColor="#696969"
                app:isTracked="true"
                app:textMovedEnable="true"
                app:trackColor="#f4f4f4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="± 5"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="21dp"
            android:background="@color/graylight" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="GSM :"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />

            <com.moos.library.HorizontalProgressView
                android:id="@+id/progressbar_gsm"
                android:layout_width="200dp"
                android:layout_height="40dp"
                app:start_color="#3bb143"
                app:end_color="#0b6623"
                android:layout_gravity="center"
                app:track_width="20dp"
                app:corner_radius="10dp"
                app:end_progress="74"
                app:progressTextColor="#696969"
                app:isTracked="true"
                app:textMovedEnable="true"
                app:trackColor="#f4f4f4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="± 5"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="21dp"
            android:background="@color/graylight" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="Color :"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />

            <com.moos.library.HorizontalProgressView
                android:id="@+id/progressbar_color"
                android:layout_width="200dp"
                android:layout_height="40dp"
                app:start_color="#3bb143"
                app:end_color="#0b6623"
                android:layout_gravity="center"
                app:track_width="20dp"
                app:corner_radius="10dp"
                app:end_progress="99"
                app:progressTextColor="#696969"
                app:isTracked="true"
                app:textMovedEnable="true"
                app:trackColor="#f4f4f4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="14.2dp"
                android:gravity="bottom"
                android:layout_weight="1"
                android:fontFamily="@font/sfpro_display_semibold"
                android:text="± 5"
                android:textColor="@color/blacklight"
                android:textSize="15.5sp" />


        </LinearLayout>

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/graylight"
        android:layout_marginTop="20.7dp"/>



</LinearLayout>

</com.fabricmonde.salesapp.customscrollview.ScrollViewWithListener>
